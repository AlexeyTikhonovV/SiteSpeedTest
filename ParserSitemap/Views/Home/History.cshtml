@model IEnumerable<ParserSitemap.Models.UrlSite>

<div class="row">
    <div class="col-md-12">
        <div class="row form-group">
            <div class="col-md-6">
                <h3>Site</h3>
            </div>
        </div>
        @foreach (var s in Model)
        {
            <div>
                <a asp-controller="Home" asp-action="Details" asp-route-SiteUrl="@s.SiteUrl">
                    <div class="row form-group">
                        <div class="col-md-6">
                            @s.SiteUrl
                        </div>
                        <hr />
                    </div>
                </a>
            </div>
        }
    </div>
</div>